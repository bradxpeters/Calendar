<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="493.0" prefWidth="400.0"
            style="-fx-background-color: #2d2d2d#2d2d2d #2d2d2d#2d2d2d;" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="appointments.AppointmentController">
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="Appointment ID" textFill="WHITE"
                               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0"/>
                        <TextField fx:id="appointmentIdTextField" disable="true" editable="false" opacity="0.5"
                                   promptText="Auto-Generated" AnchorPane.leftAnchor="120.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="Title:" textFill="WHITE" AnchorPane.leftAnchor="0.0"
                               AnchorPane.topAnchor="5.0"/>
                        <TextField fx:id="appointmentTitleTextField" AnchorPane.leftAnchor="120.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="Description:" textFill="WHITE"
                               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0"/>
                        <TextField fx:id="appointmentDescriptionTextField" AnchorPane.leftAnchor="120.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="Location:" textFill="WHITE"
                               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0"/>
                        <TextField fx:id="appointmentLocationTextField" AnchorPane.leftAnchor="120.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="Contact:" textFill="WHITE"
                               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0"/>
                        <ComboBox fx:id="appointmentContactComboBox" prefWidth="150.0" AnchorPane.leftAnchor="120.0"
                                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="Type:" textFill="WHITE" AnchorPane.leftAnchor="0.0"
                               AnchorPane.topAnchor="5.0"/>
                        <TextField fx:id="appointmentTypeTextField" AnchorPane.leftAnchor="120.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="Start Date:" textFill="WHITE"
                               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0"/>
                        <DatePicker fx:id="appointmentStartDatePicker" onAction="#ClearError"
                                    AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="0.0"
                                    AnchorPane.topAnchor="0.0"/>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="Start Time:" textFill="WHITE"
                               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0"/>
                        <HBox alignment="CENTER_LEFT" maxHeight="31.0" minHeight="31.0" prefHeight="31.0"
                              prefWidth="200.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0">
                            <children>
                                <Label text="Hour:" textFill="WHITE">
                                    <padding>
                                        <Insets right="5.0"/>
                                    </padding>
                                </Label>
                                <ComboBox fx:id="appointmentStartHourComboBox" onAction="#ClearError" prefWidth="75.0"/>
                                <Label text="Min:" textFill="WHITE">
                                    <padding>
                                        <Insets left="30.0" right="5.0"/>
                                    </padding>
                                </Label>
                                <ComboBox fx:id="appointmentStartMinuteComboBox" onAction="#ClearError"
                                          prefWidth="80.0"/>
                            </children>
                        </HBox>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="End Date:" textFill="WHITE"
                               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0"/>
                        <DatePicker fx:id="appointmentEndDatePicker" onAction="#ClearError"
                                    AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="0.0"
                                    AnchorPane.topAnchor="0.0"/>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="End Time:" textFill="WHITE"
                               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0"/>
                        <HBox alignment="CENTER_LEFT" maxHeight="31.0" minHeight="31.0" prefHeight="31.0"
                              prefWidth="200.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0">
                            <children>
                                <Label text="Hour:" textFill="WHITE">
                                    <padding>
                                        <Insets right="5.0"/>
                                    </padding>
                                </Label>
                                <ComboBox fx:id="appointmentEndHourComboBox" onAction="#ClearError" prefWidth="75.0"/>
                                <Label text="Min:" textFill="WHITE">
                                    <padding>
                                        <Insets left="30.0" right="5.0"/>
                                    </padding>
                                </Label>
                                <ComboBox fx:id="appointmentEndMinuteComboBox" onAction="#ClearError" prefWidth="80.0"/>
                            </children>
                        </HBox>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </AnchorPane>
                <AnchorPane prefWidth="200.0">
                    <children>
                        <Label layoutX="14.0" layoutY="90.0" text="Customer:" textFill="WHITE"
                               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="5.0"/>
                        <ComboBox fx:id="appointmentCustomerComboBox" prefWidth="150.0" AnchorPane.leftAnchor="120.0"
                                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </AnchorPane>
            </children>
        </VBox>
    </center>
    <bottom>
        <AnchorPane prefHeight="0.0" prefWidth="400.0" style="-fx-background-color: #2d2d2d#2d2d2d #2d2d2d#2d2d2d;"
                    BorderPane.alignment="CENTER">
            <children>
                <Separator layoutX="89.0" layoutY="13.0" prefWidth="200.0"
                           style="-fx-background-color: #2d2d2d#2d2d2d #2d2d2d#2d2d2d;" AnchorPane.leftAnchor="0.0"
                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                <Button fx:id="submitButton" layoutX="14.0" layoutY="10.0" mnemonicParsing="false"
                        onAction="#handleAppointmentSubmitButton" text="Submit" AnchorPane.bottomAnchor="9.0"
                        AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="10.0"/>
                <Button fx:id="cancelButton" layoutX="321.0" layoutY="10.0" mnemonicParsing="false"
                        onAction="#handleCancelButton" text="Cancel"/>
            </children>
        </AnchorPane>
    </bottom>
    <top>
        <HBox alignment="TOP_CENTER" prefHeight="27.0" prefWidth="400.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="errorMessageLabel" text="BLANK" textAlignment="CENTER" textFill="RED">
                    <font>
                        <Font size="15.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
    </top>
</BorderPane>
